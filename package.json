{
  "name": "chu-chu",
  "version": "0.0.0",
  "description": "RSS relay for Discord.",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node ./src/index.js --config ./nodemon.json",
    "build": "babel ./src/ -d ./public/ && cp -rf ./src/install.js ./public/install.js && cp -rf ./src/uninstall.js ./public/uninstall.js && cp -R -rf ./src/storage/ ./public/",
    "start": "babel-node ./public/install.js",
    "stop": "babel-node ./public/uninstall.js"
  },
  "keywords": [
    "RSS",
    "Discord",
    "Webhook"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "axios": "^0.19.2",
    "node-schedule": "^1.3.2",
    "node-windows": "^1.0.0-beta.1",
    "nodemon": "^2.0.4",
    "xml2js": "^0.4.23"
  }
}
